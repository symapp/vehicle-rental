<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane expanded="true" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.simao.vehicle_rental_2.controllers.DateTimeDialogController">
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
   <content>
      <GridPane hgap="10.0" prefWidth="300.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="0.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <DatePicker fx:id="dateEdit" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="268.0" GridPane.rowIndex="1" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <TextField fx:id="hourEdit" maxWidth="-Infinity" prefWidth="50.0" />
                  <Label alignment="CENTER" text=" : " textAlignment="CENTER" />
                  <TextField fx:id="minuteEdit" maxWidth="-Infinity" prefWidth="50.0" />
               </children>
            </HBox>
            <Label text="Date" />
            <Label text="Time" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </content>
</DialogPane>
